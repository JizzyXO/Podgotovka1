<Window x:Class="PodgotovkaOne.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PodgotovkaOne"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>

        <Image Source="resources/icon-cleaning.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Клининговая компания" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Grid Background="#99FF99" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="#FFA6DBFD"></Grid>
        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Content="Назад" Name="ButtonBack" Click="ButtonBack_Click" Background="#FFFFE050" Margin="614,20,31,18"/>
    </Grid>
</Window>
